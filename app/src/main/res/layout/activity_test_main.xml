<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

    <Button
        android:id="@+id/test_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="testActivity"
        />

    <Button
        android:id="@+id/test_list_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="testListActivity"
        />
    <Button
        android:id="@+id/test_expList_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="testExpListActivity"
        />
    <Button
        android:id="@+id/test_webview_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TestWebActivity"
        />
    <Button
        android:id="@+id/test_copy_activity_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TestCopytivity"
        />
</LinearLayout>
